# KetoSensei Environment Variables

# Database
DATABASE_URL="file:./dev.db"

# Flowise Integration
# Replace these with your actual Flowise instance details
FLOWISE_API_URL="http://localhost:3000"
FLOWISE_API_KEY="your-flowise-api-key-here"

# Flowise Flow IDs
# These are the specific flow IDs for different AI functions
FLOWISE_RECIPE_FLOW_ID="your-recipe-generation-flow-id"
FLOWISE_MOTIVATION_FLOW_ID="your-daily-motivation-flow-id"
FLOWISE_MEAL_PLAN_FLOW_ID="your-meal-planning-flow-id"

# Development Settings
NODE_ENV="development"

# Sentry (Error Tracking)
SENTRY_DSN="your-sentry-dsn-here"